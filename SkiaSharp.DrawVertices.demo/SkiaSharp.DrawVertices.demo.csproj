    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

	<ItemGroup>
		<None Remove="baboon.png" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SkiaSharp.Views.WPF" Version="2.88.3" />
	</ItemGroup>

	<ItemGroup>
		<Resource Include="baboon.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
	</ItemGroup>
</Project>
